{
  "name": "react-virtual-grid",
  "version": "0.0.1",
  "description": "Virtual Grid for React",
  "homepage": "http://github.com/fulcrumapp/react-virtual-grid",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "npm run lint && npm run clean && npm run build",
    "lint": "node_modules/.bin/eslint src/* src/**/*",
    "build": "npm run build-lib && npm run build-test",
    "build-lib": "node_modules/.bin/babel src --out-dir dist --source-maps",
    "build-test": "node_modules/.bin/browserify -t envify -t babelify -t reactify --extension='.jsx' -g envify -g uglifyify -s Fulcrum test/index.js > dist/test.js",
    "watch": "node_modules/.bin/watchify --verbose -t envify -t babelify -t reactify --extension='.jsx' -g envify -s Fulcrum index.js -o dist/build.js",
    "watch-test": "node_modules/.bin/watchify --verbose -t envify -t babelify -t reactify --extension='.jsx' -g envify -s Fulcrum test/index.js -o dist/test.js",
    "clean": "rm -rf dist && mkdir -p dist",
    "serve": "./node_modules/.bin/serve -p 4001",
    "check-deps": "node_modules/.bin/ncu",
    "update-deps": "node_modules/.bin/ncu -u"
  },
  "author": "Zac McCormick <zac.mccormick@gmail.com> (http://github.com/zhm)",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git://github.com/fulcrumapp/react-virtual-grid.git"
  },
  "eslintConfig": {
    "extends": "fulcrum"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.3",
    "babel-plugin-css-in-js": "^1.2.2",
    "babel-plugin-transform-proto-to-assign": "^6.6.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-es2015-node5": "^1.2.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.7.2",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "chai": "^3.5.0",
    "envify": "^3.4.0",
    "eslint": "~2.9.0",
    "eslint-config-fulcrum": "^1.0.0",
    "eslint-plugin-react": "^5.0.1",
    "glob": "^7.0.3",
    "minifyify": "^7.3.3",
    "npm-check-updates": "^2.6.3",
    "reactify": "^1.1.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "serve": "^1.4.0",
    "should": "^8.3.1",
    "source-map-support": "^0.4.0",
    "to-source": "^2.0.3",
    "uglify-js": "^2.6.2",
    "uglifyify": "^3.0.1",
    "watchify": "^3.7.0"
  },
  "peerDependencies": {
    "react": "^15.0.1",
    "react-dom": "^15.0.1"
  },
  "dependencies": {
    "classnames": "^2.2.4"
  },
  "keywords": [
    "grid",
    "table",
    "data",
    "tableview"
  ]
}
